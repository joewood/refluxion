{
  "name": "refluxion",
  "version": "0.0.5",
  "description": "Experimental Tool to generate Reudx and GraphQL classes from TypeScript model",
  "bin": {
    "refluxion": "bin/refluxion"
  },
  "scripts": {
    "test": "node lib/refluxion --interfaces --sequelize --graphql --client-ql --normalizr src/test/test-model.ts -o src/test/refluxion"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/refluxion.git"
  },
  "keywords": [
    "redux",
    "typescript",
    "graphql"
  ],
  "files": [
    "tsconfig.json",
    "src",
    "bin",
    "lib",
    "typings",
    "README.md",
    "typings.json",
    "LICENSE"
  ],
  "author": "Joe Wood",
  "license": "MIT",
  "devDependencies": {
    "@types/commander": "2.3.30",
    "@types/fs-extra": "0.0.33",
    "@types/lodash": "4.14.37",
    "@types/node": "6.0.43",
    "@types/normalizr": "2.0.17",
    "normalizr": "2.2.1",
    "tslint": "3.15.1"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "fs-extra": "^0.30.0",
    "lodash": "^4.16.0",
    "source-map-support": "^0.4.2",
    "ts-type-info": "^6.0.4",
    "typescript": "2.0.3"
  }
